{
  "version": 1,
  "disable_existing_loggers": true,
  "root": {
    "level": "DEBUG",
    "handlers": [
      "consoleHandler",
      "logFileHandler"
    ]
  },
  "handlers": {
    "consoleHandler": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "consoleFormatter",
      "stream": "ext://sys.stdout"
    },
    "logFileHandler": {
      "class": "logging.handlers.TimedRotatingFileHandler",
      "level": "DEBUG",
      "formatter": "logFileFormatter",
      "filename": "C:/StockBot/log/AppLog.log",
      "when": "midnight",
      "interval": 1,
      "backupCount": 30,
      "encoding": "utf-8"
    }
  },
  "formatters": {
    "consoleFormatter": {
      "format": "[%(levelname)-8s]%(funcName)s - %(message)s"
    },
    "logFileFormatter": {
      "format": "%(asctime)s|%(levelname)-8s|%(name)s|%(funcName)s|%(message)s"
    }
  },
  "CreonLoggingInfo":
  {
    "logFileNameArrayCreon": ["C:/StockBot/log/CreonLog_01.log"
                            ,"C:/StockBot/log/CreonLog_02.log"
                            ,"C:/StockBot/log/CreonLog_03.log"
                            ,"C:/StockBot/log/CreonLog_04.log"
                            ,"C:/StockBot/log/CreonLog_05.log"
                            ,"C:/StockBot/log/CreonLog_06.log"
                            ,"C:/StockBot/log/CreonLog_07.log"
                            ,"C:/StockBot/log/CreonLog_08.log"
                            ,"C:/StockBot/log/CreonLog_09.log"
                            ,"C:/StockBot/log/CreonLog_10.log"
                            ,"C:/StockBot/log/CreonLog_11.log"
                            ,"C:/StockBot/log/CreonLog_12.log"
                            ,"C:/StockBot/log/CreonLog_13.log"
                            ,"C:/StockBot/log/CreonLog_14.log"
                            ,"C:/StockBot/log/CreonLog_15.log"],
    "logLevel":
    {
      "SET_VALUE": 20,
      "DEBUG": 10,
      "INFO": 20,
      "WARNING": 30,
      "ERROR": 40,
      "CRITICAL": 50
    }
  }
}